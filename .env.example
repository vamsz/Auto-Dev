# Copy this file to .env and fill in your values

# Groq API Configuration (FREE - Get key at https://console.groq.com/keys)
GROQ_API_KEY="your_groq_api_key"

# Groq Model Options (uncomment one):
# - llama-3.3-70b-versatile  (best for coding, 128k context)
# - llama-3.1-8b-instant     (faster, good for simple tasks)
# - mixtral-8x7b-32768       (good balance)
# - gemma2-9b-it             (Google's model)
GROQ_MODEL=llama-3.3-70b-versatile

# GitHub Configuration (Personal Access Token with 'repo' scope)
GITHUB_TOKEN="your_github_token"

# Docker Configuration
DOCKER_IMAGE=python:3.10-slim
DOCKER_TIMEOUT=60

# Agent Configuration
MAX_RETRY_ATTEMPTS=3
WORK_DIR=./workspace
